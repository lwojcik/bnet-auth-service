{
  "name": "bnet-auth-service",
  "version": "1.0.0",
  "private": true,
  "description": "Battle.net authentication service",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "build": "npm run lint && npm run test && npm run clean && tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint --project tsconfig.json",
    "watch": "tsc-watch --onSuccess \"npm start\"",
    "dev": "npm run watch | pino-pretty",
    "test": "jest",
    "clearTestCache": "jest --clearCache",
    "start": "node scripts/start.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lukemnet/bnet-auth-service.git"
  },
  "keywords": [
    "battlenet",
    "bnet",
    "authentication",
    "api",
    "backend"
  ],
  "author": "Łukasz Wójcik",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lukemnet/bnet-auth-service/issues"
  },
  "homepage": "https://github.com/lukemnet/bnet-auth-service#readme",
  "dependencies": {
    "blizzapi": "1.1.7",
    "fastify": "2.15.0",
    "fastify-blipp": "2.3.1",
    "fastify-caching": "5.0.0",
    "fastify-env": "1.0.1",
    "fastify-ioredis": "0.1.1",
    "fastify-log": "1.2.1",
    "fastify-plugin": "1.6.1",
    "fastify-redis": "4.0.1"
  },
  "devDependencies": {
    "@types/jest": "26.0.3",
    "@types/node": "14.0.13",
    "fastify-redis-mock": "1.0.0",
    "jest": "26.0.1",
    "pino-pretty": "4.0.0",
    "rimraf": "3.0.2",
    "ts-jest": "26.1.0",
    "tsc-watch": "4.2.8",
    "tslint": "6.1.2",
    "tslint-config-airbnb": "5.11.2",
    "tslint-eslint-rules": "5.4.0",
    "tslint-immutable": "6.0.1",
    "typescript": "3.9.5"
  }
}
