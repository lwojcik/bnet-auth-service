{
  "name": "bnet-auth-service",
  "version": "1.0.0",
  "description": "Battle.net authentication microservice",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "compile": "tsc --project tsconfig.json",
    "build": "npm run lint && npm run test && npm run clean && npm run compile",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "watch": "tsc-watch --onSuccess \"npm start\"",
    "dev": "npm run watch | pino-pretty",
    "test": "jest",
    "clearTestCache": "jest --clearCache",
    "start": "node scripts/start.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sc2pte/bnet-auth-service.git"
  },
  "keywords": [
    "battlenet",
    "bnet",
    "authentication",
    "api",
    "backend"
  ],
  "author": "Łukasz Wójcik",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sc2pte/bnet-auth-service/issues"
  },
  "homepage": "https://github.com/sc2pte/bnet-auth-service#readme",
  "dependencies": {
    "@types/jest": "27.4.0",
    "@types/node": "17.0.8",
    "blizzapi": "2.1.0",
    "eslint": "8.6.0",
    "eslint-config-airbnb-typescript-prettier": "5.0.0",
    "eslint-plugin-jest": "25.7.0",
    "fastify": "3.25.3",
    "fastify-blipp": "3.1.0",
    "fastify-caching": "6.2.0",
    "fastify-env": "2.1.1",
    "fastify-ioredis": "0.1.1",
    "fastify-log": "1.2.1",
    "fastify-plugin": "3.0.0",
    "fastify-redis": "4.3.3",
    "fastify-redis-mock": "1.1.0",
    "jest": "27.4.7",
    "pino-pretty": "7.3.0",
    "prettier": "^2.5.1",
    "rimraf": "3.0.2",
    "ts-jest": "27.1.3",
    "tsc-watch": "4.6.0",
    "typescript": "4.5.4"
  }
}
