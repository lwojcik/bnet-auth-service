{
  "name": "bnet-auth-service",
  "version": "1.0.0",
  "description": "Battle.net authentication microservice",
  "keywords": [
    "battlenet",
    "bnet",
    "authentication",
    "api",
    "backend"
  ],
  "homepage": "https://github.com/sc2pte/bnet-auth-service#readme",
  "bugs": {
    "url": "https://github.com/sc2pte/bnet-auth-service/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sc2pte/bnet-auth-service.git"
  },
  "license": "MIT",
  "author": "Łukasz Wójcik",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run lint && npm run test && npm run clean && npm run compile",
    "checkFormat": "prettier --check .",
    "clean": "rimraf dist/*",
    "clearTestCache": "jest --clearCache",
    "compile": "tsc --project tsconfig.json",
    "dev": "npm run watch | pino-pretty",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "eslint . --ext .ts",
    "prepare": "husky install",
    "prepublishOnly": "npm run build",
    "start": "node scripts/start.js",
    "test": "jest",
    "watch": "tsc-watch --onSuccess \"npm start\""
  },
  "lint-staged": {
    "package.json": "sort-package-json",
    "*.ts": "eslint --cache --fix"
  },
  "dependencies": {
    "@types/jest": "28.1.1",
    "@types/node": "17.0.39",
    "blizzapi": "2.1.1",
    "eslint": "8.17.0",
    "eslint-config-lukemnet": "1.0.1",
    "eslint-plugin-jest": "26.5.1",
    "fastify": "3.29.0",
    "fastify-blipp": "3.1.0",
    "fastify-caching": "6.3.0",
    "fastify-env": "2.2.0",
    "fastify-ioredis": "0.1.1",
    "fastify-log": "1.2.1",
    "fastify-plugin": "3.0.1",
    "fastify-redis": "4.4.0",
    "fastify-redis-mock": "1.1.0",
    "husky": "8.0.1",
    "jest": "28.1.0",
    "lint-staged": "13.0.0",
    "pino-pretty": "8.0.0",
    "prettier": "2.6.2",
    "rimraf": "3.0.2",
    "sort-package-json": "1.57.0",
    "ts-jest": "28.0.5",
    "tsc-watch": "5.0.3",
    "typescript": "4.7.3"
  }
}
